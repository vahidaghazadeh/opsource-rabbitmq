{
    "name": "opsource/rabbitmq",
    "type": "library",
    "description": "RabbitMQ client laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "vahid aghazade",
            "email": "v.opsource@gmail.com",
            "role": "Developer",
            "homepage": "https://github.com/vahidaghazadeh"
        }
    ],
    "homepage": "https://github.com/vahidaghazadeh/queryadapter",
    "keywords": ["amqp","laravel","rabbitmq"],
    "require": {
        "php": ">=8.2.0",
        "ext-json": "*",
        "illuminate/support": "^9.0|^10.0|11.*",
        "php-amqplib/php-amqplib": "^v3.6.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^v8.21.1",
        "phpunit/phpunit": "^10.1",
        "squizlabs/php_codesniffer": "^3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Opsource\\Rabbitmq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opsource\\Rabbitmq\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Opsource\\Rabbitmq\\RabbitmqServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
